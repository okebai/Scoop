@model PageModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@Model.Title</title>
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,700,600,800,300" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/lib.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Site.min.css")" />
    @RenderSection("Styles", required: false)
</head>
<body>

    <div class="container-fluid">
        @RenderBody()
    </div>
    @RenderSection("OutsideContainer", required: false)

    <script type="text/javascript" src="@Url.Content("~/Scripts/lib.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/script.min.js")"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>